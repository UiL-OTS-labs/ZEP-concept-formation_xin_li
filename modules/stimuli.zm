// Item record used for the test phase item table.
record TestItem
{
    int         id;             // item im
    string      sndfn_stimulus;          // stimulus sound filename
    string      sndfn_feedback_correct;          // stimulus sound filename
    string      sndfn_feedback_incorrect;          // stimulus sound filename
    int         expected_answer;
}


